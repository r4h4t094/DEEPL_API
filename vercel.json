{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/translate",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "server.js"
    }
  ]
}
